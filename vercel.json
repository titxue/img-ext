{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/test_results/(.*)",
      "destination": "/test_results/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}